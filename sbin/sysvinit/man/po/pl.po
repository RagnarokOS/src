#. {{{  warning-message
#. type: Plain text
#: ../shutdown.8:137
#, fuzzy
#| msgid "shutdown"
msgid "When to shutdown."
msgstr "shutdown"

#. type: IP
#: ../shutdown.8:137
#, no-wrap
msgid "I<warning message>"
msgstr ""

