#, no-wrap
msgid "UPDATE-PASSWD"
msgstr ""

#. type: TH
#: ../update-passwd.8:1
#, no-wrap
msgid "Debian tools"
msgstr ""

#. type: TH
#: ../update-passwd.8:1
#, no-wrap
msgid "DEBIAN"
