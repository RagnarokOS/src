msgstr ""

#: src/stty.c:643
msgid "on"
msgstr "ჩართული"

#: src/stty.c:643
msgid "off"
msgstr "გამორთული"

#: src/stty.c:644
msgid "   ispeed N      set the input speed to N\n"
msgstr ""

#: src/stty.c:648
msgid " * line N        use line discipline N\n"
msgstr ""

#: src/stty.c:652
msgid ""
"   min N         with -icanon, set N characters minimum for a completed "
