#: ../man/free.1:141 ../man/w.1:63
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: ../man/free.1:144
msgid "Print help."
msgstr "Hilfe ausgeben."

