msgstr "B<ps\\ -ejH>"

#. type: Plain text
#: ../man/ps.1:125
msgid "B<ps\\ axjf>"
msgstr "B<ps\\ axjf>"

#. type: TP
#: ../man/ps.1:125
#, no-wrap
msgid "To get info about threads:"
msgstr "För att fi information om trådar:"

#. type: Plain text
#: ../man/ps.1:128
msgid "B<ps\\ -eLf>"
msgstr "B<ps\\ -eLf>"

#. type: Plain text
#: ../man/ps.1:130
msgid "B<ps\\ axms>"
msgstr "B<ps\\ axms>"

#. type: TP
#: ../man/ps.1:130
#, no-wrap
msgid "To get security info:"
msgstr "För att få säkerhetsinformation:"

#. type: Plain text
#: ../man/ps.1:133
msgid "B<ps\\ -eo euser,ruser,suser,fuser,f,comm,label>"
msgstr "B<ps\\ -eo euser,ruser,suser,fuser,f,comm,label>"

#. type: Plain text
#: ../man/ps.1:135
msgid "B<ps\\ axZ>"
msgstr "B<ps\\ axZ>"

#. type: Plain text
#: ../man/ps.1:137
msgid "B<ps\\ -eM>"
msgstr "B<ps\\ -eM>"

#. type: TP
#: ../man/ps.1:137
#, no-wrap
msgid "To see every process running as root (real\\ &\\ effective\\ ID) in user format:"
msgstr "För att se alla processer som kör som root (verkligt & effektivt ID) i användarformat:"

#. type: Plain text
#: ../man/ps.1:140
msgid "B<ps\\ -U\\ root\\ -u\\ root\\ u>"
msgstr "B<ps\\ -U\\ root\\ -u\\ root\\ u>"

#. type: TP
#: ../man/ps.1:140
#, no-wrap
msgid "To see every process with a user-defined format:"
msgstr "För att se alla processer med ett användardefinierat format:"

#. type: Plain text
#: ../man/ps.1:143
msgid "B<ps\\ -eo\\ pid,tid,class,rtprio,ni,pri,psr,pcpu,stat,wchan:14,comm>"
msgstr "B<ps\\ -eo\\ pid,tid,class,rtprio,ni,pri,psr,pcpu,stat,wchan:14,comm>"

#. type: Plain text
#: ../man/ps.1:145
msgid "B<ps\\ axo\\ stat,euid,ruid,tty,tpgid,sess,pgrp,ppid,pid,pcpu,comm>"
msgstr "B<ps\\ axo\\ stat,euid,ruid,tty,tpgid,sess,pgrp,ppid,pid,pcpu,comm>"

#. type: Plain text
#: ../man/ps.1:147
msgid "B<ps\\ -Ao\\ pid,tt,user,fname,tmout,f,wchan>"
msgstr "B<ps\\ -Ao\\ pid,tt,user,fname,tmout,f,wchan>"

#. type: TP
#: ../man/ps.1:147
#, no-wrap
msgid "Print only the process IDs of syslogd:"
msgstr "Skriv endast ut process-ID:n för syslogd:"

