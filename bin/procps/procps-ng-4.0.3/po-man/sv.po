msgstr "B<ps\\ -e>"

#. type: Plain text
#: ../man/ps.1:111
msgid "B<ps\\ -ef>"
msgstr "B<ps\\ -ef>"

#. type: Plain text
#: ../man/ps.1:113
msgid "B<ps\\ -eF>"
msgstr "B<ps\\ -eF>"

#. type: Plain text
#: ../man/ps.1:115
msgid "B<ps\\ -ely>"
msgstr "B<ps\\ -ely>"

#. type: TP
#: ../man/ps.1:115
#, no-wrap
msgid "To see every process on the system using BSD syntax:"
msgstr "För att se alla processer på systemed med BSD-syntax:"

#. type: Plain text
#: ../man/ps.1:118
msgid "B<ps\\ ax>"
msgstr "B<ps\\ ax>"

#. type: Plain text
#: ../man/ps.1:120
msgid "B<ps\\ axu>"
msgstr "B<ps\\ axu>"

#. type: TP
#: ../man/ps.1:120
#, no-wrap
msgid "To print a process tree:"
msgstr "För att skriva ut ett processträd:"

