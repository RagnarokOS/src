#. type: Plain text
#: ../man/uptime.1:49
msgid "display version information and exit"
msgstr ""

#. type: TP
#: ../man/uptime.1:50 ../man/w.1:89
#, no-wrap
msgid "I</var/run/utmp>"
msgstr ""

#. type: Plain text
#: ../man/uptime.1:53 ../man/w.1:92
msgid "information about who is currently logged on"
msgstr ""

#. type: TP
#: ../man/uptime.1:53 ../man/w.1:92
#, no-wrap
msgid "I</proc>"
msgstr ""

#. type: Plain text
