msgstr "  -q, --quiet          bez wypisywania ustawianej zmiennej\n"

#: src/sysctl.c:250
msgid "  -w, --write          enable writing a value to variable\n"
msgstr "  -w, --write          włączenie zapisu wartości do zmiennej\n"

